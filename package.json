{
  "name": "echodial-api",
  "version": "1.0.0",
  "description": "EchoDial API built using Node.js and Express",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch './**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "db:migrate:latest:dev": "knex migrate:latest --env development",
    "generate-types:dev": "NODE_ENV=development ts-node scripts/generate-types-from-db.ts",
    "lint": "eslint ./src --fix --ext .ts",
    "pretty-quick": "pretty-quick"
  },
  "author": "EchoDial LLC",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.425.0",
    "@types/multer": "^1.4.7",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "fast-csv": "^4.3.6",
    "husky": "^8.0.3",
    "jsonwebtoken": "^9.0.1",
    "knex": "^2.4.2",
    "knex-types": "^0.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.1",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.5",
    "stripe": "^14.3.0",
    "twilio": "^4.12.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.6.1",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
